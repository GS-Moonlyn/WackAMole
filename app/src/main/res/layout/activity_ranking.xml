<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Ranking">

    <Button
        android:id="@+id/btnVoltar"
        android:layout_width="143dp"
        android:layout_height="65dp"
        android:text="Voltar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.95" />

    <TextView
        android:id="@+id/txt1Lugar"
        android:layout_width="150dp"
        android:layout_height="20dp"
        android:layout_marginTop="76dp"
        android:text="1ยบ Lugar:"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.071"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txt2Lugar"
        android:layout_width="150dp"
        android:layout_height="20dp"
        android:layout_marginTop="24dp"
        android:text="2ยบ Lugar:"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.071"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txt1Lugar" />

    <TextView
        android:id="@+id/txt3Lugar"
        android:layout_width="150dp"
        android:layout_height="20dp"
        android:layout_marginTop="28dp"
        android:text="3ยบ Lugar:"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.071"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txt2Lugar" />

</androidx.constraintlayout.widget.ConstraintLayout>